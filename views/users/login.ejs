<%- include('../partials/head') %>
<%- include('../partials/header') %>

<h3 class="form-title">Ingresar</h3>

<form action="/" method="POST" id="form-register" name="form-register">
    <div>
        <input type="text" name="email" id="email" placeholder="Email">
    </div>

    <div>
        <input type="text" name="contraseña" id="contraseña" placeholder="Contraseña">
    </div>

    <div>
      <p><a href="/forgotPassword">¿Has olvidado la contraseña?</a></p>
      <br>
    </div>

    <div id="buttons">
        <button type="reset" class="login-registro" onclick = "location='/register'">Registrarse</button>
        <button type="submit" class="login-ingresar">Ingresar</button>
    </div>

</form>

<%- include('../partials/linkWhatsApp') %>
<%- include('../partials/footer') %>
